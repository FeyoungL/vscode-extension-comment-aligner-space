{
	"name": "comment-aligner-space",
	"displayName": "comment aligner space",
	"description": "A tool for aligning the inline trailing comment, forked from febaoshan/vscode-extension-comment-aligner",
	"version": "1.0.2",
	"publisher": "Feyoung",
	"icon": "icon/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/feyoungl/vscode-extension-comment-aligner-space.git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.commentaligner"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.commentaligner",
				"title": "Comment Aligner Space"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"eslint": "^4.11.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"vsce": "^2.9.1"
	}
}
